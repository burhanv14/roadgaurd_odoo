{
  "name": "roadguard-backend",
  "version": "1.0.0",
  "description": "RoadGuard Backend API with Authentication",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node src/server.ts",
    "dev:nodemon": "nodemon --exec ts-node src/server.ts",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "type-check": "tsc --noEmit",
    "test:email": "npm run build && node test-email.js",
    "test:auth": "node test-auth.js",
    "db:sync": "ts-node src/scripts/syncDatabase.ts",
    "db:sync:build": "npm run build && node dist/scripts/syncDatabase.js",
    "test:backend": "node test-backend.js"
  },
  "keywords": [
    "roadguard",
    "api",
    "authentication",
    "express",
    "sequelize"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/multer": "^2.0.0",
    "@types/nodemailer": "^7.0.1",
    "@types/uuid": "^10.0.0",
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^2.7.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.5",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.2",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.4",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/node": "^20.8.0",
    "@types/pg": "^8.10.7",
    "nodemon": "^3.0.2",
    "sequelize-cli": "^6.6.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  }
}
